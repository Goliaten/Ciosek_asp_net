@model IEnumerable<Kategoria>
@{
    ViewData["Title"] = "Home Page";
    List<int> lista = new List<int>{ 1, 2, 3, 4, 5 };
    string color;
}

<div class="text-center">
    @foreach (var kategoria in Model)
    {
        <a href="@Url.Action("Lista", "Filmy", new{nazwaKategorii = kategoria.Nazwa})">@kategoria.Nazwa</a>
    }
</div>
